.title span{
  text-align: center;
  border: 3px solid #ccc;
  border-radius: 6px;
  padding: 5px;
  color: black;
  font-size: 26px;
}


.users{
  .col-md-6{
    text-align: center;
    img{
      margin-bottom: 50px;
    }
  }
}

.title{
  margin: 30px 0;
  text-align: center;
}

.wrap_2{
  padding-left: 25px;
}

.sprite {
    background-image: url(<%= asset_path 'spritesheet.png' %>);
    background-repeat: no-repeat;
    display: block;
}

.sprite-calendar, .sprite-diary, .sprite-goal, .sprite-list{
  display: inline-block;
  margin-left: 15px;
  margin-top: 15px;
}

.sprite-calendar{
    width: 131px;
    height: 125px;
    background-position: -1px -1px;
}

.sprite-diary{
    width: 134px;
    height: 125px;
    background-position: -134px -1px;
}

.sprite-goal{
    width: 125px;
    height: 125px;
    background-position: -270px -1px;
}

.sprite-list{
    width: 91px;
    height: 125px;
    background-position: -397px -1px;
}